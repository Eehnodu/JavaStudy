package ifelseë¬¸;

import java.util.Scanner;

public class asd {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String a = sc.next();
        String b = "";
        int tmp;
        
        for(int i = 0 ; i < a.length(); i++){
            
            tmp = (int)a.charAt(i);
            
            if(tmp >=65 && tmp <=90){
                b += (char)(tmp+ 32);
            }
            else(tmp >=97 && tmp <= 122) {
                b += (char)(tmp -32);
            }
        }
        System.out.print(b);
    }
}